{
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.3",
    "vite-plugin-shopify": "^3.0.1"
  },
  "type": "module",
  "scripts": {
    "logout": "shopify auth logout",
    "dev": "npm run logout && run-p -sr \"shopify:dev -- {@}\" \"vite:dev\" --",
    "deploy": "run-s \"vite:build\" \"shopify:push -- {@}\" --",
    "shopify:dev": "shopify theme dev -e development",
    "shopify:push": "shopify theme push -e development -d",
    "shopify:pull": "shopify theme pull -e development -o=*.json",
    "vite:dev": "vite",
    "vite:build": "vite build"
  },
  "dependencies": {
    "sass": "^1.77.7"
  }
}
